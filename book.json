{
  "title": "前端从业者心得总结",
  "description": "写给自己看的心得体会",
  "author": "ab了c",
  "output.name": "docs",
  "language": "zh-hans",
  "root": "/src",
  "structure": {
    "readme": "readme.md",
    "summary": "summary.md"
  },
  "script": {
    "build": "gitbook build ./ ./docs"
  },
  "plugins": [
    "hide-element",
    "chapter-fold",
    "code",
    "highlight",
    "donate",
    "-livereload",
    "-sharing",
    "-lunr", 
    "-search"
  ],
  "pluginsConfig": {
    "hide-element": {
      "elements": [
        ".gitbook-link",
        ".divider",
        ".book-header"
      ]
    },
    "donate": {
      "wechat": "./img/wechat.png",
      "title": "你的打赏将用于支撑此网站的运行",
      "button": "打赏",
      "wechatText": "微信打赏"
    }
  }
}